<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>zQueue_Assignment__c</fullName>
    <description>This formula field is used to filter reports to show each individual user&#39;s zQueue.</description>
    <externalId>false</externalId>
    <formula>IF(CONTAINS( Owner:Queue.QueueName ,  $User.LastName ), TRUE, FALSE)</formula>
    <label>zQueue Assignment</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
